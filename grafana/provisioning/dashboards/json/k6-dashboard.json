          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT MAX(rps) AS \"Peak RPS\" FROM (SELECT SUM(m.metric_value) / ${interval_value} AS rps FROM k6_metrics m JOIN tests t ON t.id = m.test_id JOIN domains d ON d.id = t.domain_id WHERE d.name = '${domain}' AND t.name = '${test}' AND m.metric_name = 'http_reqs' AND $__timeFilter(m.timestamp) GROUP BY floor(extract(epoch FROM m.timestamp) / ${interval_value})) sub",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "purple", "value": null }
