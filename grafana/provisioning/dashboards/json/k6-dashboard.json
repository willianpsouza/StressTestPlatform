          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT COALESCE(SUM(CASE WHEN m.status NOT IN ('200', '201') THEN m.metric_value ELSE 0 END) * 100.0 / NULLIF(SUM(m.metric_value), 0), 0) AS \"Error Rate\" FROM k6_metrics m JOIN tests t ON t.id = m.test_id JOIN domains d ON d.id = t.domain_id WHERE d.name = '${domain}' AND t.name = '${test}' AND m.metric_name = 'http_reqs' AND $__timeFilter(m.timestamp)",
