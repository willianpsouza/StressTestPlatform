      "id": 1,
      "title": "Request Made",
      "type": "stat",
      "gridPos": { "h": 10, "w": 6, "x": 0, "y": 0 },
      "datasource": { "type": "postgres", "uid": "stresstest-postgres" },
      "targets": [
        {
          "datasource": { "type": "postgres", "uid": "stresstest-postgres" },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT SUM(m.metric_value) AS \"Requests\" FROM k6_metrics m JOIN tests t ON t.id = m.test_id JOIN domains d ON d.id = t.domain_id WHERE d.name = '${domain}' AND t.name = '${test}' AND m.metric_name = 'http_reqs' AND $__timeFilter(m.timestamp)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
