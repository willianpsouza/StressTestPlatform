          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT r.time, COALESCE(r.reqs / NULLIF(v.vus, 0), 0) AS \"Req/VU\" FROM (SELECT to_timestamp(floor(extract(epoch FROM m.timestamp) / ${interval_value}) * ${interval_value}) AS time, SUM(m.metric_value) AS reqs FROM k6_metrics m JOIN tests t ON t.id = m.test_id JOIN domains d ON d.id = t.domain_id WHERE d.name = '${domain}' AND t.name = '${test}' AND m.metric_name = 'http_reqs' AND $__timeFilter(m.timestamp) GROUP BY 1) r LEFT JOIN (SELECT to_timestamp(floor(extract(epoch FROM m.timestamp) / ${interval_value}) * ${interval_value}) AS time, MAX(m.metric_value) AS vus FROM k6_metrics m JOIN tests t ON t.id = m.test_id JOIN domains d ON d.id = t.domain_id WHERE d.name = '${domain}' AND t.name = '${test}' AND m.metric_name = 'vus' AND $__timeFilter(m.timestamp) GROUP BY 1) v ON r.time = v.time ORDER BY r.time",
